{
  "type": "object",
  "properties": {
    "vasp_code": {
      "type": "string",
      "minLength": 1
    },
    "callback_permission_request_url": {
      "type": "string",
      "format": "uri"
    },
    "callback_txid_url": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string",
          "format": "uri"
        }
      ]
    },
    "signature": {
      "type": "string",
      "minLength": 128,
      "maxLength": 128,
      "pattern": "^[0123456789A-Fa-f]+$"
    }
  },
  "anyOf": [
    {
      "required": ["vasp_code", "callback_permission_request_url", "signature"]
    },
    {
      "required": ["vasp_code", "callback_txid_url", "signature"]
    }
  ],
  "additionalProperties": false
}
